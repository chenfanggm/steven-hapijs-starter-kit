  "scripts": {
    "dev": "better-npm-run dev",
    "start": "better-npm-run start",
    "forever": "better-npm-run start",
    "test": "better-npm-run test",
    "test:cov": "better-npm-run test:cov",
    "cluster": "better-npm-run cluster",
    "lint": "eslint --ext .js ."
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon bin/start.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/start.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "cluster": {
      "command": "node bin/cluster.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "boom": "^7.1.1",
    "debug": "^3.1.0",
    "good": "^7.3.0",
    "good-console": "^6.4.0",
    "good-file": "^6.0.1",
    "good-http": "^6.1.3",
    "good-squeeze": "^5.0.2",
    "good-winston": "^4.0.0",
    "hapi": "^16.6.2",
    "inert": "^4.2.1",
    "joi": "^13.0.2",
    "nes": "^7.0.1",
    "request": "^2.83.0",
    "rotating-file-stream": "^1.3.4",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  },
  "devDependencies": {
    "better-npm-run": "^0.1.0",
    "chai": "^4.1.2",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0"
  }
}
